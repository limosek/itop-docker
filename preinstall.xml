<?xml version="1.0" encoding="UTF-8"?>
<installation>
  <mode>$ITOPMODE</mode>
  <source_dir>datamodels/2.x/</source_dir>
  <database>
    <server>$DBHOST</server>
    <port>$DBORT</port>
    <name>$DBNAME</name>
    <user>$DBUSER</user>
    <pwd>$DBPASSWORD</pwd>
    <db_tls_enabled></db_tls_enabled>
    <db_tls_ca></db_tls_ca>
    <prefix></prefix>
  </database>
  <graphviz_path>/usr/bin/dot</graphviz_path>
  <language>$ITOPLANG</language>
  <url>$ITOPURL</url>

  <admin_account>
    <user>$ITOPADMIN</user>
    <pwd>$ITOPPASSWORD</pwd>
    <language>$ITOPLANG</language>
  </admin_account>

  <selected_modules type="array">
		<module>itop-structure</module>
                <module>authent-local</module>
                <module>itop-attachments</module>
                <module>itop-welcome-itil</module>
                <module>itop-profiles-itil</module>
                <module>itop-config-mgmt</module>
                <module>itop-service-mgmt</module>
                <module>itop-tickets</module>
  </selected_modules>
  <sample_data></sample_data>
         
</installation>
