<?xml version="1.0" encoding="UTF-8"?>
<installation>
  <mode>$ITOPMODE</mode>
  <source_dir>datamodels/2.x/</source_dir>
  <datamodel_version>$ITOP_VERSION</datamodel_version>
  <database>
    <server>$DBHOST</server>
    <port>$DBORT</port>
    <name>$DBNAME</name>
    <user>$DBUSER</user>
    <pwd>$DBPASSWORD</pwd>
    <db_tls_enabled></db_tls_enabled>
    <db_tls_ca></db_tls_ca>
    <prefix></prefix>
  </database>
  <graphviz_path>/usr/bin/dot</graphviz_path>
  <language>$ITOPLANG</language>
  <url>$ITOPURL</url>

  <admin_account>
    <user>$ITOPADMIN</user>
    <pwd>$ITOPPASSWORD</pwd>
    <language>$ITOPLANG</language>
  </admin_account>

  <selected_modules type="array">
   <item>authent-local</item>
  <item>itop-config</item>
  <item>itop-structure</item>
  <item>itop-attachments</item>
  <item>itop-core-update</item>
  <item>itop-portal-base</item>
  <item>molkobain-handy-framework</item>
  <item>teemip-framework</item>
  <item>teemip-ip-mgmt</item>
  <item>teemip-ipv6-mgmt</item>
  <item>itop-welcome-itil</item>
  </selected_modules>
  
  <sample_data></sample_data>
         
</installation>
